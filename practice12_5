public class Main {
    public static void main(String[] args) {
        boolean flag1 = true, flag2 = false;
        System.out.println("flag1 && flag2: " + (flag1 && flag2)); // false
        System.out.println("flag1 || flag2: " + (flag1 || flag2)); // true
        System.out.println("!flag1: " + (!flag1));                // false
    }
}
